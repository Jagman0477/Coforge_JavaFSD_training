package com.mine.testing;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class Test2 {

	public static int fked[][];
	
	public static void createBoard() {
		int right = 8;
		int down = 8;
		
		fked = new int[right][down];
		
		int temp = 0;
		
		for(int i = 0; i < right; i++) {
			for(int j = 0; j < down; j++) {
				fked[i][j] = temp;
				temp++;
			}
		}
		
		for(int i = 0; i < right; i++) {
			for(int j = 0; j<down; j++, temp++) {
				System.out.print(fked[i][j]+" ");
			}
		}
	}
	
	public static int[] returnIndex(int a) {
		int[] res = new int[2];
		
		for(int i=0; i<8; i++) {
			for(int j = 0; j<8; j++) {
				if(fked[i][j] == a) {
					res[0] = i;
					res[1] = j;
					System.out.println("\n"+i+" "+j);
					return res;
				}
			}
		}
		return null;
	}
	
	public static void main(String[] args) {
		createBoard();
		int[] temp = returnIndex(62);
		List<Integer> l = new ArrayList(Arrays.asList(temp));
		System.out.println("\n"+l);
	}
	
}
